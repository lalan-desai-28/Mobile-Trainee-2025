<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <!-- Internet -->
    <uses-permission android:name="android.permission.INTERNET"/>

    <!-- Location -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" /> <!-- Gallery -->
    <uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="32" />
    <uses-permission android:name="android.permission.READ_MEDIA_IMAGES" /> <!-- Camera -->
    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Android_Learning"
        tools:targetApi="31">
        <activity
            android:name=".webview.WebViewLearning"
            android:exported="false" />
        <activity
            android:name=".sharedpreferences.DashboardActivity"
            android:exported="false" />
        <activity
            android:name=".sharedpreferences.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".permissions.RuntimePermissionsLearning"
            android:exported="false" />
        <activity
            android:name=".intent.activities.ImageIntent"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.SEND" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="image/jpeg" />
                <data android:mimeType="image/png" />
            </intent-filter>
        </activity>
        <activity
            android:name=".intent.activities.AudioIntent"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.SEND" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="audio/mpeg" />
            </intent-filter>
        </activity>
        <activity
            android:name=".intent.IntentLearning"
            android:exported="false" />
        <activity
            android:name=".recyclerview.RecyclerViewLearning"
            android:exported="false" />
        <activity
            android:name=".fragments.FragmentsLearning"
            android:exported="false" />
        <activity
            android:name=".dialog.DialogLearning"
            android:exported="false" />
        <activity
            android:name=".font.FontLearning"
            android:exported="false" />
        <activity
            android:name=".snackbar_fab.fab.FABExample"
            android:exported="false" />
        <activity
            android:name=".snackbar_fab.snackbar.SnackbarExamples"
            android:exported="false" />
        <activity
            android:name=".snackbar_fab.SnakckbarAndFABLearning"
            android:exported="false" />
        <activity
            android:name=".appbar_toolbar.collapsing.CollapsingToolbarAndCoordinatorLayout"
            android:exported="false" />
        <activity
            android:name=".appbar_toolbar.toolbar.SimpleToolbar"
            android:exported="false" />
        <activity
            android:name=".appbar_toolbar.actionbar.SimpleActionBar"
            android:exported="false"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".appbar_toolbar.AppbarToolbarLearning"
            android:exported="false" />
        <activity android:name=".viewpager.ViewPagerActivity" />
        <activity
            android:name=".dimensions.responsive_blog.ResponsiveBlogActivity"
            android:exported="false" />
        <activity
            android:name=".dimensions.DimensionsLearning"
            android:exported="false" />
        <activity
            android:name=".drawable.DrawableActivity"
            android:exported="false" />
        <activity
            android:name=".layout.design.DesignActivity"
            android:exported="false" />
        <activity
            android:name=".layout.profile.ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".layout.signup.SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".layout.LayoutLearning"
            android:exported="false" />
        <activity
            android:name=".activity.ActivityLearning"
            android:exported="false" />
        <activity
            android:name=".activity.data_pass.SecondActivity"
            android:exported="false" />
        <activity
            android:name=".activity.lifecycle.LifeCycleActivity"
            android:exported="false" />
        <activity
            android:name=".activity.data_pass.DataPassActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>